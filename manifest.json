{
    "manifest_version": 2,
    "name": "Files", 
    "version": "2.0", 
    "icons": {
        "16": "/images/16.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["content.js"]}
    ], 
    "permissions": [
        "tabs", 
        "webRequests"
    ]
}